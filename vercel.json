{
  "version": 2,
  "name": "apptravel",
  "rewrites": [
    {
      "source": "/((?!public/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/public/Audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        },
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        }
      ]
    },
    {
      "source": "/public/Images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}
